{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"es2020",
			"DOM",
			"WebWorker"
		],
		"moduleResolution": "node",
		"module": "es2020",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2020",
		"noImplicitAny": false,
		"paths": {
			"$src": [
				"src"
			],
			"$src/*": [
				"src/*"
			],
			"$components": [
				"src/components"
			],
			"$components/*": [
				"src/components/*"
			],
			"$lib": [
				"src/lib"
			],
			"$lib/*": [
				"src/lib/*"
			],
			"$generated": [
				".svelte-kit/types/src"
			],
			"$generated/*": [
				".svelte-kit/types/src/*"
			]
		}
	}
}